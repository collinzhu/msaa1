{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false
    

  },
  "jsx-no-lambda":false,
  "object-literal-sort-keys": false,
  "prefer-const": ["error", {
    "destructuring": "any",
    "ignoreReadBeforeAssign": false
}],
"enum": [
  "array",
  "generic",
  "array-simple"
]
}
